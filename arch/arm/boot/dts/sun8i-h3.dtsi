			resets = <&ccu RST_UART0>;
			dmas = <&dma 6>, <&dma 6>;
			dma-names = "rx", "tx";
#endif
			status = "disabled";
		};
#if 1 // test pb start
		uart1: serial@01c28400 {
			compatible = "snps,dw-apb-uart";
